{
  "name": "nodejs-dev-container",
  "version": "1.0.0",
  "description": "Build VSCode docker container for nodejs development",
  "private": true,
  "scripts": {
    "build": "docker build --file .devcontainer/Dockerfile --tag creativefeather/nodejs-dev:base .",
    "commit": "docker commit -a \"creativefeather <creativefeather@outlook.com>\" creativefeather/nodejs-dev:base creativefeather/nodejs-dev:latest",
    "push": "docker push creativefeather/nodejs-dev:latest",
    "git:file": "git checkout nodejs --",
    "git:orphan": "git checkout --orphan"
  },
  "author": "\"creativefeather\" <\"creativefeather@outlook.com\">",
  "license": "MIT"
}